################################################################################
# OS generated files
#   - ignore OS generated files
################################################################################
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
desktop.ini


################################################################################
# Eclipse
#   - ignore Eclipse specific files
################################################################################
.project
.settings


################################################################################
# Node
#   - ignore the root node_modules
#   - and any other node_modules
################################################################################
node_modules/*
*/node_modules/*


################################################################################
# build
#   - ignore build results, but keep the directory
#   - ignore workspace files, but keep the directory
################################################################################
build/*
!build/.gitignore
work/*
!work/.gitignore


################################################################################
# logs
#   - ignore log files
################################################################################
*.log
